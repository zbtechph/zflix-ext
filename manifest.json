{
    "manifest_version": 3,
    "name": "ZFlix",
    "version": "0.1.0",
    "description": "Sit down and flix.",
    "permissions": [
        "cookies"
    ],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/favicon-16x16.png",
            "32": "images/favicon-32x32.png"
        }
    }
}